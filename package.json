{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "webpack": "umi webpack",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "play": "swc playground.tsx -o playground.js && node playground.js"
  },
  "types": "./src/components/types/types.tsx",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "axios": "^0.26.0",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "react-icons": "^4.8.0",
    "react-tag-input": "^6.8.1",
    "recharts": "^2.1.9",
    "redux-logger": "^3.0.6",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redux-logger": "^3.0.9",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
